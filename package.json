{
  "name": "@godspeedsystems/godspeed",
  "description": "Godspeed CLI",
  "homepage": "https://godspeed.systems",
  "version": "2.7.5alpha-1",
  "main": "./lib/index.js",
  "bin": {
    "godspeed": "./lib/index.js"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "build:prod": "tsc --project tsconfig.json",
    "build:dev": "tsc --project tsconfig.json --sourceMap --watch",
    "prepublishOnly": "npm run clean && npm run build:prod",
    "local": "sudo npm uninstall -g && sudo npm install -g && godspeed"
  },
  "dependencies": {
    "@godspeedsystems/core": "latest",
    "axios": "^0.27.2",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^10.0.1",
    "docker-compose": "^0.23.17",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "es6-template-string": "^1.2.0",
    "fs-extra": "^11.1.1",
    "glob": "10.3.4",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "pino-pretty": "^10.2.0",
    "replace-in-file": "^6.3.5",
    "signale": "^1.4.0",
    "simple-git": "^3.7.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.1",
    "@types/glob": "^7.2.0",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/inquirer": "^9.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.17.14",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "cli",
    "nodejs",
    "godspeed",
    "typescript"
  ]
}