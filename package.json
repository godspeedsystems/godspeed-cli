{
  "name": "godspeed-cli",
  "description": "Godspeed CLI",
  "version": "1.0",
  "main": "./lib/index.js",
  "bin": {
    "godspeed": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "local": "sudo npm uninstall -g && sudo npm install -g && godspeed",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "sudo npm i -g && godspeed"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^7.1.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7",
    "prompt-sync": "^4.2.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.14.33",
    "@types/prompt-sync": "^4.1.1",
    "depcheck": "^1.4.0",
    "js-yaml": "^4.1.0",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.7",
    "simple-git": "^3.7.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "cli",
    "nodejs",
    "godspeed",
    "typescript"
  ]
}
