{
  "name": "@godspeedsystems/godspeed",
  "description": "Godspeed CLI",
  "version": "2.1.0",
  "main": "./lib/index.js",
  "bin": {
    "godspeed": "./lib/index.js"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "build:prod": "tsc --project tsconfig.json",
    "build:dev": "tsc --project tsconfig.json --sourceMap",
    "prepublishOnly": "npm run clean && npm run build:prod",
    "local": "npm run build:dev && sudo npm uninstall -g && sudo npm install -g && godspeed"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^7.1.0",
    "docker-compose": "^0.23.17",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "figlet": "^1.5.0",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "path": "^0.12.7",
    "prompt-sync": "^4.2.0",
    "replace-in-file": "^6.3.5",
    "simple-git": "^3.7.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/figlet": "^1.5.4",
    "@types/glob": "^7.2.0",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@types/prompt-sync": "^4.1.1",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "cli",
    "nodejs",
    "godspeed",
    "typescript"
  ]
}